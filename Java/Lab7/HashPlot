	public static void hash(int[] arr) {
 // a *k + b mod m
		
		for (int i = 0; i < arr.length; i++) {
		    arr[i] = (arr[i] *31 + 8);
		}
		
		String k = args[0];
		String[] kArr = k.split(" ");
		int[] intArray = new int[kArr.length]; 
		for(int i = 0; i < kArr.length; i++) {
		    intArray[i] = Integer.parseInt(kArr[i]);
		    
		    
	}

